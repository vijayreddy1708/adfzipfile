		"emailblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'emailblob'"
		},
		"error_SQL_connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'error_SQL_connection'"
