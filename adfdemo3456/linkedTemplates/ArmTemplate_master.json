		"scdblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'scdblob'"
		},
		"sqllnk_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqllnk'"
		},
		"srclink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srclink'"
